<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.tipoReservacion.tipoReservacionFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="30dp"
        android:orientation="vertical">

        <Button
            android:id="@+id/botonAgregarTipoReservacionF"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/agregarTipoReservacion"
            />

        <Button
            android:id="@+id/botonConsultarTipoReservacionF"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/consultarTipoReservacion"
            />

        <Button
            android:id="@+id/botonActualizarTipoReservacionF"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/actualizarTipoReservacion"
            />

        <Button
            android:id="@+id/botonEliminarTipoReservacionF"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/eliminarTipoReservacion"
            />

    </LinearLayout>

</FrameLayout>